{
  "version": 2,
  "functions": { "api/index.js": { "maxDuration": 30 } },
  "routes": [
    { "src": "^/app\\.js$", "dest": "/public/app.js", "headers": { "Content-Type": "application/javascript; charset=utf-8" } },
    { "src": "^/api/(.*)$", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
